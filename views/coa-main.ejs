        <div class="content-wrapper coa-body coa-area">
            <div class="col-md-10 col-centered">
                <section class="content-header">
                    <h1 class="coa-body-header">Certificate of Analysis</h1>
                </section>
            </div>
            <section class="content">
                <div class="row">
                    <div class="col-md-10 col-centered">
                        <div class="nav-tabs-custom">
                            <ul class="nav nav-tabs nav-tab-design">
                                <li class="active">
                                    <a href="#kitting" data-toggle="tab">Kitting</a>
                                </li>
                                <li class="">
                                    <a href="#qa" data-toggle="tab">QA File</a>
                                </li>
                                <li class="">
                                    <a href="#download" data-toggle="tab">Download</a>
                                </li>
                            </ul>
                            <div class="tab-content">
                                <div class="tab-pane active" id="kitting">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div id="responseDiv">
                                                <!-- response here-->
                                            </div>
                                            <form id="uploadboxid">
                                                <div id="toHideIfSuccess">
                                                    <input type="hidden" name="authenticity_token" value="<%= authenticity_token %>">
                                                    <h4 class="coa-body-form">CoA Box ID & Runcard</h4>
                                                    <label class="coa-form-label">Box ID:</label>
                                                    <div class="form-group has-feedback">
                                                        <input type="text" autofocus="on" name="boxid" id="boxid" class="form-control" required onchange="document.getElementById('responseDiv').style.display = 'none';">
                                                    </div>
                                                    <label class="coa-form-label">Tag Runcard</label>
                                                    <div class="form-group has-feedback">
                                                        <section id="multiRow">
                                                            <div id="initRow">
                                                                <input name="multifield" id="runcardno" class="form-control form-kitting-interface" onchange="document.getElementById('responseDiv').style.display = 'none';" style="display:inline; width:94%;">
                                                            </div>
                                                        </section>
                                                    </div>
                                                    <br>
                                                    <div class="row">
                                                        <div class="col-xs-7">
                                                            <div class="coa-sub-link">
                                                                By tagging "4 runcards" consecutively, this form will automatically be submitted.
                                                            </div>
                                                        </div>
                                                        <div class="col-xs-5">
                                                            <button type="submit" id="submitkitting" class="btn btn-primary btn-block btn-success btn-flat">Submit</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                        <div class="col-md-8">
                                            <h4 class="coa-body-form">Recent activity</h4>
                                            <ul class="timeline timeline">
                                                <% for(let i=0; i<data.kitting.length; i++) { %>
                                                    <li>
                                                        <i class="fa fa-cloud-upload bg-gray">
                                                        </i>
                                                        <div class="timeline-item">
                                                            <span class="time">
                                                                <i class="fa fa-clock-o"></i> <%= data.kitting[i].upload_date %>&nbsp;
                                                                <button type="button" class="btn btn-link btn-flat dropdown-toggle" data-toggle="dropdown">
                                                                    <span class="fa fa-ellipsis-v"></span>
                                                                </button>
                                                                <ul class="dropdown-menu coa-dropdown" role="menu">
                                                                    <li>
                                                                        <a href="#edit" data-toggle="modal" data-target="#modalwarning<%= data.kitting[i].id %>">Edit</a>
                                                                    </li>
                                                                    <li>
                                                                        <a href="#delete" data-toggle="modal" data-target="#modaldelete<%= data.kitting[i].id %>">Remove</a>
                                                                    </li>
                                                                </ul>
                                                            </span>
                                                            <h3 class="timeline-header"><%= data.kitting[i].runcard %> has been tagged to <%= data.kitting[i].box_id %></h3>
                                                            <div class="timeline-body"> Transaction id: <%= data.kitting[i].id %>, uploaded by <%= data.kitting[i].username %>. </div>
                                                            <div class="timeline-footer">
                                                            </div>
                                                        </div>
                                                    </li>
                                                <% } %>
                                                <li>
                                                    <i class="fa fa-clock-o bg-gray"></i>
                                                </li>
                                            </ul>
                                        </div>
                                        <!---->
                                        <% for(let i=0; i<data.kitting.length; i++) { %>
                                        <div class="modal modal-default" id="modaldelete<%= data.kitting[i].id %>">
                                            <div class="modal-dialog modal-settings">
                                                <div class="modal-content">
                                                    <div class="modal-header">
                                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                            <span aria-hidden="true">&times;</span>
                                                        </button>
                                                        <h4 class="modal-title">Think twice! You cannot undo this action.</h4>
                                                    </div>
                                                    <div class="modal-body">
                                                        <p>Are you sure you want to <span class="label label-danger">DELETE</span> this CoA details?</p>
                                                        <table class="table table-striped">
                                                            <tbody>
                                                                <thead>
                                                                <tr>
                                                                    <th>ID</th>
                                                                    <th>Upload Date</th>
                                                                    <th>Box Id</th>
                                                                    <th>Runcard</th>
                                                                    <th>Username</th>
                                                                </tr>
                                                                </thead>
                                                                <tr>
                                                                    <td><%= data.kitting[i].id %></td>
                                                                    <td><%= data.kitting[i].upload_date %></td>
                                                                    <td><%= data.kitting[i].box_id %></td>
                                                                    <td><%= data.kitting[i].runcard %></td>
                                                                    <td><%= data.kitting[i].username %></td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                    <div class="modal-footer">
                                                        <form id="deletecoadetails<%= data.kitting[i].id %>">
                                                            <input type="hidden" name="authenticity_token" value="<%= authenticity_token %>">
                                                            <input type="hidden" name="deleteById" value="<%= data.kitting[i].id %>">
                                                            <input type="hidden" name="deleteByUsername" value="<%= data.kitting[i].username %>">
                                                            <div id="responseDivModal<%= data.kitting[i].id %>">
                                                                <!-- response here-->
                                                            </div>
                                                            <div id="toHideButtonIfSuccess<%= data.kitting[i].id %>">
                                                                <button type="button" class="btn btn-default btn-flat " data-dismiss="modal" aria-label="Close" >Cancel</button>
                                                                <button type="submit" id="deletebykitting<%= data.kitting[i].id %>" class="btn btn-danger btn-flat">Yes, Delete</button>
                                                            </div>
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <% } %>
                                        <!---->
                                        <% for(let i=0; i<data.kitting.length; i++) { %>
                                        <div class="modal modal-default" id="modalwarning<%= data.kitting[i].id %>">
                                            <div class="modal-dialog modal-settings">
                                                <div class="modal-content">
                                                    <div class="modal-header">
                                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                            <span aria-hidden="true">&times;</span>
                                                        </button>
                                                        <h4 class="modal-title">Edit transaction id <%= data.kitting[i].id %> created by <%= data.kitting[i].username %> </h4>
                                                    </div>
                                                    <form id="editcoadetails<%= data.kitting[i].id %>">
                                                        <div class="modal-body">
                                                                <input type="hidden" name="authenticity_token" value="<%= authenticity_token %>">
                                                                <input type="hidden" name="edit_id" value="<%= data.kitting[i].id %>">
                                                                <input type="hidden" name="edit_username" value="<%= data.kitting[i].username %>">
                                                                <label class="coa-modal-label">Box ID:</label>
                                                                <input type="text" class="form-control form-kitting-interface" id="edit_boxid<%= data.kitting[i].id %>" required name="edit_boxid" placeholder="<%= data.kitting[i].box_id %>" value="<%= data.kitting[i].box_id %>">
                                                                <br>
                                                                <label class="coa-modal-label">Runcard:</label>
                                                                <input type="text" class="form-control form-kitting-interface" id="edit_runcard<%= data.kitting[i].id %>" required name="edit_runcard" placeholder="<%= data.kitting[i].runcard %>" value="<%= data.kitting[i].runcard %>">
                                                        </div>
                                                        <div class="modal-footer">
                                                            <div id="responseDivEditModal<%= data.kitting[i].id %>">
                                                                <!-- response here-->
                                                            </div>
                                                            <div id="toHideButtonEditIfSuccess<%= data.kitting[i].id %>">
                                                                <button type="button" class="btn btn-default btn-flat " data-dismiss="modal" aria-label="Close" >Cancel</button>
                                                                <button type="submit" id="editbykitting<%= data.kitting[i].id %>" class="btn btn-warning btn-flat">Submit Changes</button>
                                                            </div>
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>
                                        <% } %>
                                    </div>
                                </div>
                                <div class="tab-pane" id="qa">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div id="responseDivQA">
                                                <!-- response here-->
                                            </div>
                                            <form id="uploadcoa">
                                                <div id="toHideIfSuccess">
                                                    <input type="hidden" name="authenticity_token" value="<%= authenticity_token %>">
                                                    <h4 class="coa-body-form">Upload CoA File</h4>
                                                    <label class="coa-form-label">Excel file:</label>
                                                    <div class="form-group has-feedback">
                                                        <input type="file" name="coafile" id="coafile" required style="width:100%" >
                                                    </div>
                                                    <label class="coa-form-label">Invoice:</label>
                                                    <div class="form-group has-feedback">
                                                        <input type="text" autocomplete="off" name="invoice" id="invoice" class="form-control form-coa-interface" required onchange="document.getElementById('responseDivQA').style.display = 'none';">
                                                    </div>
                                                    <label class="coa-form-label">Delivery Date:</label>
                                                    <div class="form-group has-feedback">
                                                        <div class="input-group date">
                                                            <div class="input-group-addon form-coa-date-interface ">
                                                                <i class="fa fa-calendar"></i>
                                                            </div>
                                                            <input type="text" autocomplete="off" name="datepicker" id="datepicker" class="form-control form-coa-interface pull-right" required>
                                                        </div>
                                                    </div>
                                                    <label class="coa-form-label">Supplier:</label>
                                                    <div class="form-group has-feedback">
                                                        <select class="form-control select2 select2-hidden-accessible  form-coa-interface" style="width:100%" tabindex="-1" aria-hidden="true" required name="supplier" id="supplier" >
                                                            <option selected="selected" value="1007">ACC</option>
                                                            <option value="1003">FERROTEC</option>
                                                            <option value="1001">TZS</option>
                                                            <option value="1005">ACMK</option>
                                                            <option value="1006">LONGI</option>
                                                            <option disabled>NORSUN</option>
                                                        </select>
                                                    </div>
                                                    <br>
                                                    <div class="row">
                                                        <div class="col-xs-7">
                                                            <div class="coa-sub-link">
                                                                By clicking "Submit", you indicate that all fields are true and correct.
                                                            </div>
                                                        </div>
                                                        <div class="col-xs-5">
                                                            <button type="submit" id="submitcoafile" class="btn btn-primary btn-block btn-success btn-flat">Submit</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                        <div class="col-md-8">
                                            <h4 class="coa-body-form">Recent activity</h4>
                                            <ul class="timeline timeline-invers">
                                                <% for(let i=0; i<data.qa.length; i++) { %>
                                                <li>
                                                    <i class="fa fa-cloud-upload bg-gray">
                                                    </i>
                                                    <div class="timeline-item">
                                                        <span class="time">
                                                            <i class="fa fa-clock-o"></i> <%= data.qa[i].date %>
                                                            <!-- hide for now as we dont have means to delete/edit using primary key / invoice without using special characters 
                                                            <button type="button" class="btn btn-link btn-flat dropdown-toggle" data-toggle="dropdown">
                                                                <span class="fa fa-ellipsis-v"></span>
                                                            </button>
                                                            <ul class="dropdown-menu coa-dropdown" role="menu">
                                                                <li>
                                                                    <a href="#edit" data-toggle="modal" data-target="#modalwarning<%= data.qa[i].invoice %>">Edit</a>
                                                                </li>
                                                                <li>
                                                                    <a href="#delete" data-toggle="modal" data-target="#modaldelete<%= data.qa[i].invoice %>">Remove</a>
                                                                </li>
                                                            </ul>
                                                            -->
                                                        </span>
                                                        <h3 class="timeline-header"><%= data.qa[i].invoice %> has been uploaded</h3>
                                                        <div class="timeline-body">Delivery date: <%= data.qa[i].delivery_date %> | Supplier: <%= data.qa[i].supplier %> | uploaded by <%= data.qa[i].username %></div>
                                                        <div class="timeline-footer"></div>
                                                    </div>
                                                </li>
                                                <% } %>
                                                <li>
                                                    <i class="fa fa-clock-o bg-gray"></i>
                                                </li>
                                            </ul>
                                        </div>
                                        <!---->
                                        <% for(let i=0; i<data.qa.length; i++) { %>
                                        <div class="modal modal-default" id="modaldelete<%= data.qa[i].invoice %>">
                                            <div class="modal-dialog modal-settings">
                                                <div class="modal-content">
                                                    <div class="modal-header">
                                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                            <span aria-hidden="true">&times;</span>
                                                        </button>
                                                        <h4 class="modal-title">Think twice! You cannot undo this action.</h4>
                                                    </div>
                                                    <div class="modal-body">
                                                        <p>Are you sure you want to <span class="label label-danger">DELETE</span> this CoA details?</p>
                                                        <table class="table table-striped">
                                                            <tbody>
                                                                <thead>
                                                                <tr>
                                                                    <th>Upload Date</th>
                                                                    <th>Delivery Date</th>
                                                                    <th>Invoice</th>
                                                                    <th>Supplier</th>
                                                                    <th>Username</th>
                                                                </tr>
                                                                </thead>
                                                                <tr>
                                                                    <td><%= data.qa[i].date %></td>
                                                                    <td><%= data.qa[i].delivery_date %></td>
                                                                    <td><%= data.qa[i].invoice %></td>
                                                                    <td><%= data.qa[i].supplier %></td>
                                                                    <td><%= data.qa[i].username %></td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                    <div class="modal-footer">
                                                        <form id="deletecoadetails<%= data.qa[i].invoice %>">
                                                            <input type="hidden" name="authenticity_token" value="<%= authenticity_token %>">
                                                            <input type="hidden" name="deleteByInvoice" value="<%= data.qa[i].invoice %>">
                                                            <input type="hidden" name="deleteByUsername" value="<%= data.qa[i].username %>">
                                                            <div id="responseDivModal<%= data.qa[i].invoice %>">
                                                                <!-- response here-->
                                                            </div>
                                                            <div id="toHideButtonIfSuccess<%= data.qa[i].invoice %>">
                                                                <button type="button" class="btn btn-default btn-flat " data-dismiss="modal" aria-label="Close" >Cancel</button>
                                                                <button type="submit" id="deletebyqa<%= data.qa[i].invoice %>" class="btn btn-danger btn-flat">Yes, Delete</button>
                                                            </div>
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <% } %>
                                        <!---->
                                        <% for(let i=0; i<data.qa.length; i++) { %>
                                        <div class="modal modal-default" id="modalwarning<%= data.qa[i].invoice %>">
                                            <div class="modal-dialog modal-settings">
                                                <div class="modal-content">
                                                    <div class="modal-header">
                                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                            <span aria-hidden="true">&times;</span>
                                                        </button>
                                                        <h4 class="modal-title">Edit invoice <%= data.qa[i].invoice %> created by <%= data.qa[i].username %> </h4>
                                                    </div>
                                                    <form id="editcoadetails<%= data.qa[i].invoice %>">
                                                        <div class="modal-body">
                                                                <input type="hidden" name="authenticity_token" value="<%= authenticity_token %>">
                                                                <input type="hidden" name="edit_invoice" value="<%= data.qa[i].invoice %>">
                                                                <input type="hidden" name="edit_username" value="<%= data.qa[i].username %>">
                                                                <label class="coa-modal-label">Delivery date:</label>
                                                                <div class="input-group date">
                                                                    <div class="input-group-addon form-coa-date-interface ">
                                                                        <i class="fa fa-calendar"></i>
                                                                    </div>
                                                                    <input type="text" autocomplete="off" id="modalDatepicker<%= data.qa[i].invoice %>" class="form-control form-coa-interface" id="edit_delivery_date<%= data.qa[i].delivery_date %>" required name="edit_delivery_date" placeholder="<%= data.qa[i].delivery_date %>" value="<%= data.qa[i].delivery_date %>">
                                                                </div>
                                                                <br>
                                                                <label class="coa-modal-label">Invoice:</label>
                                                                <input type="text" class="form-control form-coa-interface" id="edit_invoice<%= data.qa[i].invoice %>" required name="edit_invoice" placeholder="<%= data.qa[i].invoice %>" value="<%= data.qa[i].invoice %>">
                                                        </div>
                                                        <div class="modal-footer">
                                                            <div id="responseDivEditModal<%= data.qa[i].invoice %>">
                                                                <!-- response here-->
                                                            </div>
                                                            <div id="toHideButtonEditIfSuccess<%= data.qa[i].invoice %>">
                                                                <button type="button" class="btn btn-default btn-flat " data-dismiss="modal" aria-label="Close" >Cancel</button>
                                                                <button type="submit" id="editbykitting<%= data.qa[i].invoice %>" class="btn btn-warning btn-flat">Submit Changes</button>
                                                            </div>
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>
                                        <% } %>
                                    </div>
                                </div>
                                <div class="tab-pane" id="download">
