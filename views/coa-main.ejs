<!-- KITTING Upload script -->
<script>
    window.addEventListener('load', function(){

        function goUpload(){

            let xhr = new XMLHttpRequest();
            let fd = new FormData(form); // bind form data object and form element
            
            xhr.addEventListener('load', function(event){
                console.log(event.target.response);
                let resFromServer = JSON.parse(event.target.response);
                document.getElementById('responseDiv').style.display='block';

                function resDiv(){
                    let responseDiv = document.getElementById('responseDiv');
                    
                    if(resFromServer.err){

                        if (responseDiv.classList) { 
                            responseDiv.classList.add("responseDivErr");
                            responseDiv.innerHTML = resFromServer.err
                        } else {
                            // For IE9
                            var classes = responseDiv.className.split(" ");
                            var i = classes.indexOf("responseDivErr");

                            if (i >= 0) {
                                classes.splice(i, 1);
                            } else { 
                                classes.push("responseDivErr");
                                responseDiv.className = classes.join(" "); 
                                responseDiv.innerHTML = resFromServer.err
                            }
                        }

                    } else {

                        if (responseDiv.classList) { 
                            responseDiv.classList.add("responseDivSuccess");
                            responseDiv.innerHTML = resFromServer.auth;

                            
                            setTimeout(function(){
                                window.location.href="/coa";
                            }, 50);

                        } else {
                            // For IE9
                            var classes = responseDiv.className.split(" ");
                            var i = classes.indexOf("responseDivSuccess");

                            if (i >= 0) {
                                classes.splice(i, 1);
                            } else { 
                                classes.push("responseDivSuccess");
                                responseDiv.className = classes.join(" "); 
                                responseDiv.innerHTML = resFromServer.auth;
                                
                                setTimeout(function(){
                                    window.location.href="/coa";
                                }, 50);
                            }
                        }
                    }

                    
                }

                resDiv();
            });

            xhr.addEventListener('error', function(event){
                alert('Something went wrong.');
            });

            xhr.open('POST', '/api/coakitting');
            xhr.send(fd);

        }

        // access FORM element
        let form = document.getElementById('uploadboxid');
        // take over submit event
        form.addEventListener('submit', function(event){
            event.preventDefault();
            //document.getElementById('responseDiv').style.display="none";
            goUpload();
            document.getElementById('submitkitting').setAttribute("disabled", "disabled");

        });


    });
</script>
<!-- KITTING Tag runcard script-->
<script>
    $('document').ready(function(){

        $('#boxid').keydown(function(e){
            if(e.keyCode == 13){
                if($('#boxid').val()){
                    $('#runcardno').focus();
                    return false;
                }
            }
        });

        function addRow(section, clone){

            let newRow = clone.removeAttr('id').addClass('new').insertBefore(initRow);
            let deleteRow = $('<a class="rowDelete"><i class="fa fa-close"></i></a>');

            newRow
            .append(deleteRow)
            .on('click', 'a.rowDelete', function(){
                removeRow(newRow);
            })
            .slideDown(300, function(){
                $(this).find('input').focus();
            })
            .keydown(function(e){
                if(e.keyCode == 13){
                    $('#initRow').find('input').focus();
                    return false;
                }
            });
        }

        function removeRow(newRow){
            newRow
            .slideUp(200, function(){
                $(this)
                    .next('div:not(#initRow)')
                    .find('input').focus()
                    .end()
                    .remove();
            });
        }

        $(function(){
            let initRow = $('#initRow')
            let section = initRow.parent('section');
            let i = 0;

            initRow.on('focus', 'input', function(){
                i++;

                if(i<5){
                    let clone = initRow.clone();
                    clone.attr("id", "runcardno");
                    clone.find("#runcardno").attr("name", 'runcard'+i);
                    addRow(section, clone);
                } else {
                    
                    document.getElementsByName('multifield')[0].setAttribute("disabled", "disabled");

                    if($('#boxid').val()){
                        document.getElementById('submitkitting').click();
                        document.getElementById('submitkitting').setAttribute("disabled", "disabled");
                        return 0;
                    }
                }

            });

        });

    });
</script>
