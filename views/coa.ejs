<!-- Upload script -->
<script>
    window.addEventListener('load', function(){

        function goUpload(){

            let xhr = new XMLHttpRequest();
            let fd = new FormData(form); // bind form data object and form element
            
            xhr.addEventListener('load', function(event){
                console.log(event.target.response);
                let resFromServer = JSON.parse(event.target.response);
                document.getElementById('responseDiv').style.display='block';
                function resDiv(){
                    let responseDiv = document.getElementById('responseDiv');
                    
                    if(resFromServer.err){
                        document.getElementById('coafile').removeAttribute("disabled");
                        document.getElementById('datepicker').removeAttribute("disabled");
                        document.getElementById('invoice').removeAttribute("disabled");
                        document.getElementById('supplier').removeAttribute("disabled");
                        document.getElementById('submitcoafile').removeAttribute("disabled");

                        if (responseDiv.classList) { 
                            responseDiv.classList.add("responseDivErr");
                            responseDiv.innerHTML = resFromServer.err
                        } else {
                            // For IE9
                            var classes = responseDiv.className.split(" ");
                            var i = classes.indexOf("responseDivErr");

                            if (i >= 0) {
                                classes.splice(i, 1);
                            } else { 
                                classes.push("responseDivErr");
                                responseDiv.className = classes.join(" "); 
                                responseDiv.innerHTML = resFromServer.err
                            }
                        }

                    } else {

                        if (responseDiv.classList) { 
                            responseDiv.classList.add("responseDivUploading");
                            responseDiv.innerHTML = resFromServer.auth;
                            
                            let hideDiv = document.getElementById('toHideIfSuccess');
                                hideDiv.style.display = "none";
                            
                                
                                setTimeout(function(){
                                window.location.href="/coa/qa";
                                }, 500);
                                
                               
                        } else {
                            // For IE9
                            var classes = responseDiv.className.split(" ");
                            var i = classes.indexOf("responseDivUploading");

                            if (i >= 0) {
                                classes.splice(i, 1);
                            } else { 
                                classes.push("responseDivUploading");
                                responseDiv.className = classes.join(" "); 
                                responseDiv.innerHTML = resFromServer.auth;
                                
                                let hideDiv = document.getElementById('toHideIfSuccess');
                                    hideDiv.style.display = "none";

                                
                                setTimeout(function(){
                                window.location.href="/coa/qa";
                                }, 500);
                                
                            }
                        }
                    }

                    
                }

                resDiv();
            });

            xhr.addEventListener('error', function(event){
                alert('Something went wrong.');
            });

            xhr.open('POST', '/api/coa');
            xhr.send(fd);

        }

        // access FORM element
        let form = document.getElementById('uploadcoa');
        // take over submit event
        form.addEventListener('submit', function(event){
            event.preventDefault();
            document.getElementById('responseDiv').style.display="none";
            goUpload();
            document.getElementById('coafile').setAttribute("disabled", "disabled");
            document.getElementById('datepicker').setAttribute("disabled", "disabled");
            document.getElementById('invoice').setAttribute("disabled", "disabled");
            document.getElementById('supplier').setAttribute("disabled", "disabled");
            document.getElementById('submitcoafile').setAttribute("disabled", "disabled");
        });


    });
</script>
